contents
<table class="table table-bordered">
  <thead>
  <tr>
    <th>id</th>
    <th>image</th>
  </tr>
  </thead>
  <tbody>
    <% @contents.each do |content| %>
    <tr>
      <td><%= content.id %></td>
      <td><%= image_tag(content.image.to_s, height: '30') %></td>
    </tr>
    <% end %>
  </tbody>
</table>
